# Production Environment Configuration for Frontend

# API Configuration
REACT_APP_API_BASE_URL=https://api.github-evaluator.com/api/v1
REACT_APP_WEBSOCKET_URL=wss://api.github-evaluator.com/ws

# OAuth Configuration (Production)
REACT_APP_GITHUB_CLIENT_ID=your-production-github-client-id
REACT_APP_GOOGLE_CLIENT_ID=your-production-google-client-id

# Application Settings
REACT_APP_APP_NAME=GitHub Repository Evaluator
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=production

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true

# UI Configuration
REACT_APP_THEME=light
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_ITEMS_PER_PAGE=20

# Production Optimizations
GENERATE_SOURCEMAP=false
REACT_APP_DEBUG=false

# CDN Configuration
REACT_APP_CDN_URL=https://cdn.github-evaluator.com
REACT_APP_ASSETS_URL=https://assets.github-evaluator.com

# Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=G-XXXXXXXXXX
REACT_APP_MIXPANEL_TOKEN=your-mixpanel-token

# Error Reporting
REACT_APP_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Performance Monitoring
REACT_APP_NEW_RELIC_LICENSE_KEY=your-new-relic-license-key